<% @questions.each do |question| %>
  <div class="qa-q-list-item" id="q75295">
    <div class="qa-q-item-stats">
      <div class="qa-voting qa-voting-net" id="voting_75295">
        <div class="qa-vote-buttons qa-vote-buttons-net">
          <input title="Click to vote up" onclick="return qa_vote_click(this);" type="submit" value="+" class="qa-vote-first-button qa-vote-up-button"> 
          <input title="Click to vote down" onclick="return qa_vote_click(this);" type="submit" value="â€“" class="qa-vote-second-button qa-vote-down-button"> 
        </div>
        <div class="qa-vote-count qa-vote-count-net">
          <span class="qa-netvote-count">
          <span class="qa-netvote-count-data">0</span><span class="qa-netvote-count-pad"> votes</span>
          </span>
        </div>
        <div class="qa-vote-clear"></div>
      </div>
      <span class="qa-a-count">
        <span class="qa-a-count-data">0</span><span class="qa-a-count-pad"> answer</span>
      </span>
      <span class="qa-view-count">
        <span class="qa-view-count-data"></span><span class="qa-view-count-pad"> views</span>
      </span>
    </div>
    <div class="qa-q-item-main">
      <div class="qa-q-item-title">
        <%= link_to question.title, question %>
      </div>
      <span class="qa-q-item-meta">
        <span class="qa-q-item-when">
          Posted <%= time_ago_in_words(question.created_at) %> ago
        </span>
        <span class="qa-q-item-where">
          <span class="qa-q-item-who">
            <span class="qa-q-item-who-pad">by </span>
            <span class="qa-q-item-who-data"><a href="./user/esqeudero" class="qa-user-link">User</a></span>
          </span>
        </span>
      </span>
      <div class="qa-q-item-tags">
        <%-# put tags here-%>
      </div>
    </div>
    <div class="qa-q-item-clear"></div>
  </div>
<% end %>
